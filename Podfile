source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '7.0'

target 'HealthDrips' do
    
    # 网络库
    pod 'AFNetworking', '~> 3.0'
    
    # 图片下载
    pod 'SDWebImage'
    
    # 加载
    pod 'MBProgressHUD'

    # 下拉刷新
    pod 'MJRefresh'
    
    # 高德地图
    pod 'AMap2DMap'
    pod 'AMapSearch'
    
    # 友盟分享
    pod 'UMengSocialCOM'

end